# Fallout3Terminal Configuration
# Copy to .env and edit: cp .env.example .env

# Name displayed in the terminal greeter header
TERMINAL_NAME="Proto-Boy"

# Typewriter effect speed (characters per second). Default: 80
TYPEWRITE_SPEED=80

# Set to true to skip the boot animation and go straight to the main menu
SKIP_BOOT=false

# Set to false to disable all sound effects
SOUND_ENABLED=true

# Editor for "Edit Personal Log". If empty, uses vim then vi then nano so you get the same
# navigation as create log (arrows, Enter = new line). Set to override, e.g. nano, code -w
LOG_EDITOR=
